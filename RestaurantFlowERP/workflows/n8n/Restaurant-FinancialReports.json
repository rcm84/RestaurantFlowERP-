    {
      "id": "format-data",
      "name": "Format Report Data",
      "type": "n8n-nodes-base.code",
      "position": [
        100,
        460
      ],
      "parameters": {
        "jsCode": "// Merge all sales data\nconst sales = $('Load All Sales Data').all();\nconst tables = $('Load Table Orders').all();\n\nconst today = new Date().toISOString().split('T')[0];\nconst todaySales = sales.filter(item => item.json.timestamp?.startsWith(today));\nconst todayTables = tables.filter(item => item.json.timestamp?.startsWith(today));\n\nconst totalSales = todaySales.reduce((sum, item) => sum + (item.json.total || 0), 0);\nconst totalTables = todayTables.reduce((sum, item) => sum + (item.json.total || 0), 0);\n\nconst reportData = {\n  date: today,\n  totalSales: totalSales,\n  totalTables: totalTables,\n  grandTotal: totalSales + totalTables,\n  ordersCount: todaySales.length + todayTables.length,\n  avgTicket: (totalSales + totalTables) / (todaySales.length + todayTables.length),\n  rawData: { sales: todaySales, tables: todayTables }\n};\n\nreturn [{ json: reportData }];"
      },
      "typeVersion": 2
    },
